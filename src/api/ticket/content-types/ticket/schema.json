{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "Tickets"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string"
    },
    "company": {
      "type": "string"
    },
    "business_number": {
      "type": "biginteger"
    },
    "business_email": {
      "type": "email"
    },
    "location": {
      "type": "string"
    },
    "field": {
      "type": "string"
    },
    "enquiry": {
      "type": "text"
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "admin_verified": {
      "type": "boolean",
      "default": false
    },
    "payment_status": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "in-progress",
        "error",
        "done"
      ]
    }
  }
}
