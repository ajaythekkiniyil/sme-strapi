      "type": "text",
      "required": true
    },
    "problemStatement": {
      "type": "text",
      "required": true
    },
    "budgetRange": {
      "type": "string",
      "required": true
    },
    "urgency": {
      "type": "string",
      "required": true
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
